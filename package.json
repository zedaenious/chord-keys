{
  "name": "chord-keys",
  "version": "1.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zedzelenak/chord-keys.git"
  },
  "author": "Matt Zelenak",
  "private": true,
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "build:dev": "npx webpack --devtool source-map && webpack serve",
    "pub:minor": "npm version minor --force",
    "pub:patch": "npm version patch --force",
    "clean": "rm -rf node_modules && rm package-lock.json && rm dist/bundle.js && rm dist/bundle.js.map",
    "clean:build": "npm run clean && npm install && npm run build:dev"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^5.3.2",
    "less-loader": "^10.0.1",
    "style-loader": "^3.0.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
